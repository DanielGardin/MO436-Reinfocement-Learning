    "        history = {\n",
    "            \"cumulative reward\"  : [],\n",
    "            \"episode error\"      : [],\n",
    "            \"episode length\"     : [],\n",
    "            \"episode win\"        : []\n",
    "        }\n",
    "\n",
    "        Ntable = defaultdict(lambda : {action: 0 for action in Actions.actions})\n",
    "\n",
